{
  "lexicon": 1,
  "id": "app.wikisky.artboardAlbum",
  "description": "Artboard album (collection) names.",
  "defs": {
    "main": {
      "type": "record",
      "description": "A named artboard/album that media items can be assigned to.",
      "key": "album",
      "record": {
        "type": "object",
        "required": ["name", "createdAt"],
        "properties": {
          "name": {
            "type": "string",
            "maxLength": 256,
            "description": "Display name of the album."
          },
          "createdAt": {
            "type": "string",
            "format": "datetime",
            "description": "When the album was created."
          }
        }
      }
    }
  }
}
